$define(["wgt_util"],function(n){var i;function t(n){var t,e,o,c;t=n.breadcrumbType,e=n.contentFlag,o=parseInt(n.contentWidth),c=i.find(".p_breadcrumb"),t?"false"==e?c.removeClass("contentBox"):c.find(".p_content").css("width",o):(c.removeClass("contentBox"),c.find(".p_content").remove())}return{init:function(){var n=this._params;i=$compCl.getScope(n.id),t(n)}}});